<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="special_2017_RoundedImageView">
            <attr name="riv_left_top_corner_radius" format="dimension" />
            <attr name="riv_right_top_corner_radius" format="dimension" />
            <attr name="riv_left_bottom_corner_radius" format="dimension" />
            <attr name="riv_right_bottom_corner_radius" format="dimension" />
            <attr name="riv_border_width" format="dimension" />
            <attr name="riv_border_color" format="color" />
            <attr name="riv_oval" format="boolean" />
            <attr name="android:scaleType" />
    </declare-styleable>
    <declare-styleable name="CircleView">
        <attr name="shape" format="enum">
            <enum name="rect" value="1"/>
            <enum name="circle" value="2"/>
        </attr>
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="cover_color" format="color" />
        <attr name="round_radius" format="dimension"/>
    </declare-styleable>
</resources>